$("tr[data-type='Active']").hide("slow");
$("tr[data-type='Hold']").show("slow");
$("tr[data-type='Complete']").hide("slow");
$("li:has(a#tab_active_pill)").removeClass("active")
$("li:has(a#tab_hold_pill)").addClass("active")
$("li:has(a#tab_complete_pill)").removeClass("active")
$("li:has(a#tab_all_pill)").removeClass("active")